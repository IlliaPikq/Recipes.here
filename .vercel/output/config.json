{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_image.astro"
		},
		{
			"src": "^\\/$",
			"dest": "index.astro"
		},
		{
			"src": "^\\/recipes\\/?$",
			"dest": "recipes/index.astro"
		},
		{
			"src": "^\\/recipes\\/([^/]+?)\\/?$",
			"dest": "recipes/[id]/index.astro"
		},
		{
			"src": "^\\/create\\/?$",
			"dest": "create/index.astro"
		}
	]
}